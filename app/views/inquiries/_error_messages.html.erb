<% if @inquiry.errors.any? %>
	<div id="error_explanation">
		<div class="alert alert-error">
			The form contains <%= pluralize(@inquiry.errors.count, "error") %>.
    	</div>
		<ul>
			<% @inquiry.errors.full_messages.each do |msg| %>
				<li>* <%= msg %></li>
			<% end %>
		</ul>
	</div>
<% end %>